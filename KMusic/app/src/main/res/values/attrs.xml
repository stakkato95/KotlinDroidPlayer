<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MusicProgressBar">
        <attr name="lineColor" format="color" />
        <attr name="innerCircleColor" format="color" />
        <attr name="progressColor" format="color" />
        <attr name="barThickness" format="dimension" />
        <attr name="barThicknessTouched" format="dimension" />
        <attr name="touchTimeToStartScrollingMillis" format="integer" />
        <attr name="vibrationTime" format="integer" />
        <attr name="vibrationTimeOnFirstTouch" format="integer" />
        <attr name="angleBetweenVibrations" format="integer" />
        <attr name="angleMeasurementErrorPercent" format="float" />
    </declare-styleable>
</resources>
